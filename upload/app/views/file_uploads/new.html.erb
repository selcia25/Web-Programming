<h1>New file upload</h1>

<%= render "form", file_upload: @file_upload %>

<br>

<div>
  <%= link_to "Back to file uploads", file_uploads_path %>
</div>

<%= form_with(model: @file_upload, multipart: true) do |form| %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="field">
    <%= form.label :file %>
    <%= form.file_field :file %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>